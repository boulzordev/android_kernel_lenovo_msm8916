/dts-v1/;

#include "msm8939-v3.0.dtsi"
#include "msm8939-qrd-wt82918.dtsi"
#include "dsi-panel-hx8394f-boe-720p-video.dtsi"
#include "dsi-panel-ili9881c_otm-720p-video.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM8939v3.0 QRD SKUK";
	compatible = "qcom,msm8939-qrd-wt82918", "qcom,msm8939-qrd", "qcom,msm8939", "qcom,qrd";
	qcom,board-id = <0x1000b 18>, <0x2000b 18>;
};

&soc {
	i2c@78b9000 { /* BLSP1 QUP5 */
		focaltech@38 {
			focaltech,display-coords = <0 0 720 1280>;
			focaltech,panel-coords = <0 0 720 1280>;
		};
	};
};

&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&dsi_hx8394f_boe_720p_video>;
	qcom,dsi-pref-sub-pan = <&dsi_ili9881c_otm_720p_video>;
};

&dsi_hx8394f_boe_720p_video {
	qcom,cont-splash-enabled;
};

&dsi_ili9881c_otm_720p_video {
	qcom,cont-splash-enabled;
};

